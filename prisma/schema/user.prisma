model User {
  id Int @id @default(autoincrement())
  email String @unique
  username String @unique
  password String
  firstName String?
  lastName String? 
  isActive Boolean @default(true)
  trips Trip[]
  TripMember TripMember[]
  Expense Expense[]
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}